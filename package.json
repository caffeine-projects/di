{
  "name": "@caffeine-projects/di",
  "version": "0.0.0",
  "description": "IoC container for Javascript & Typescript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --build --force tsconfig.build.json",
    "test": "node --import tsx/esm --test-reporter spec --test **/*.test.ts",
    "test:coverage": "NODE_V8_COVERAGE=out npm test -- --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=lcov.info",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "fmt": "prettier . --write",
    "fmt:check": "prettier . --check",
    "bench": "node --expose-gc --import=tsx benchmarks/bench.ts",
    "bench:compare": "node --expose-gc --import=tsx benchmarks/bench_compare_third_parties.ts"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@loopback/context": "^7.0.7",
    "@mitata/counters": "^0.0.1",
    "@nestjs/common": "^10.4.12",
    "@nestjs/core": "^10.4.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "colorette": "^2.0.20",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.12.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "expect": "^29.7.0",
    "fastify": "^5.1.0",
    "inversify": "^6.1.5",
    "jest-mock": "^29.7.0",
    "lint-staged": "^15.2.10",
    "mitata": "^1.0.19",
    "prettier": "^3.4.2",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.0.0",
    "table": "^6.8.2",
    "tsx": "^4.19.2",
    "tsyringe": "^4.8.0",
    "typescript": "^5.7.2",
    "uuid": "^11.0.3"
  }
}
