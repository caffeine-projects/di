{
  "name": "@caffeine-projects/di",
  "version": "0.0.0",
  "description": "IoC container for Javascript & Typescript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --build --force tsconfig.build.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --detectOpenHandles",
    "test:ci": "npm test -- --colors --coverage --ci",
    "test:coverage": "npm test -- --coverage",
    "lint": "npx eslint",
    "lint:fix": "npx eslint --fix",
    "fmt": "npx prettier . --write",
    "fmt:check": "npx prettier . --check",
    "bench": "node --loader ts-node/esm _bench/bench.ts",
    "bench:compare": "node --loader ts-node/esm _bench/compare.ts"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@jest/globals": "^28.1.3",
    "@loopback/context": "^5.0.2",
    "@nestjs/common": "^9.0.8",
    "@nestjs/core": "^9.0.8",
    "@types/jest": "^28.1.6",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.6.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "colorette": "^2.0.19",
    "cronometro": "^1.1.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "fastify": "^4.4.0",
    "inversify": "^6.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "2.8.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "supertest": "^6.2.4",
    "table": "^6.8.0",
    "ts-jest": "28.0.7",
    "ts-node": "^10.9.1",
    "tsyringe": "^4.7.0",
    "typescript": "^4.9.5",
    "uuid": "^8.3.2"
  }
}
