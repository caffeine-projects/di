{
  "name": "@caffeine-projects/di",
  "version": "0.0.0",
  "description": "IoC container for Javascript & Typescript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --build --force tsconfig.build.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:ci": "jest --colors --coverage --ci",
    "test:coverage": "npm test -- --coverage",
    "test:debug": "npm test -- --detectOpenHandles",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "fmt": "prettier . --write",
    "fmt:check": "prettier . --check",
    "bench": "./bin/bench",
    "bench:compare": "./bin/bench-compare-libs"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/globals": "^29.7.0",
    "@loopback/context": "^7.0.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "colorette": "^2.0.20",
    "cronometro": "^3.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "fastify": "^4.27.0",
    "inversify": "^6.0.2",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.0.0",
    "table": "^6.8.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsyringe": "^4.8.0",
    "typescript": "^5.5.3",
    "uuid": "^9.0.1"
  }
}
